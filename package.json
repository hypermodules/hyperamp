  "description": "Indescribable music player",
  "version": "0.0.0",
  "author": "Hyper Modules (https://github.com/hypermodules)",
  "bugs": {
    "url": "https://github.com/hypermodules/hyperamp/issues"
  },
  "contributors": [
    "Bret Comnes <bcomnes@gmail.com> (http://bret.io)",
    "Nate Goldman <ungoldman@gmail.com> (http://ungoldman.com)"
  ],
  "dependencies": {
    "choo": "^3.2.0",
    "choo-location-electron": "^2.0.0",
    "choo-log": "^1.4.0",
    "csjs": "^1.0.6",
    "electron-config": "^0.2.1",
    "electron-context-menu": "^0.7.0",
    "electron-debug": "^1.0.1",
    "es6-template-strings": "^2.0.1",
    "folder-walker": "^3.0.0",
    "format-duration": "^1.0.0",
    "insert-css": "^1.1.0",
    "musicmetadata": "^2.0.4"
  },
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron": "1.4.12",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "spritesh": "^1.2.0",
    "standard": "^8.0.0",
    "svgo": "^0.7.1",
    "tape": "^4.6.0",
    "watchify": "^3.7.0"
  },
  "homepage": "https://github.com/hypermodules/hyperamp",
  "keywords": [
    "amp",
    "hyper",
    "music",
    "player"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypermodules/hyperamp.git"
  },
  "scripts": {
    "start": "electron main",
    "test": "standard",
    "prepublish": "npm run build",
    "build": "run-s clean clean-svg build-sprite clean:tmp",
    "build-sprite": "mkdirp renderer/dist && spritesh -i node_modules/.tmp -o renderer/dist/sprite.svg",
    "clean-svg": "rimraf tmp && svgo -f renderer/icons -o node_modules/.tmp",
    "clean": "run-p clean:*",
    "clean:tmp": "rimraf node_modules/.tmp",
    "clean:dist": "rimraf renderer/dist"
  }
}
